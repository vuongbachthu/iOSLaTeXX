  },
  "source": {
    "git": "https://github.com/vuongbachthu/iOSLaTeXX.git",
    "tag": "0.3.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "4.2"
  ],
  "source_files": "iOSLaTeXX/Classes/**/*",
  "resource_bundles": {
    "iOSLaTeXX": [
      "iOSLaTeXX/Assets/*",
